{
  "scssLint": {
    "config": "./gulp/config/lint-scss.yml"
  },
  "autoprefixer": {
    "browsers": ["ie 8-10", "Last 2 Chrome versions"]
  },
  "minifyCSS": {
    "compatibility": "ie8"
  },
  "minifyHTML": {
    "collapseWhitespace": true,
    "removeComments": true,
    "minifyCSS": true
  },
  "gzipOptions": {
    "append": false,
    "extension": "gz",
    "threshold": "1kb",
    "level": 9
  },
  "jshintOptions": {
    "node": true,
    "globals": {
      "app": false,
      "angular": false,
      "$": false,
      "_": false,
      "moment": false,
      "window": false
    }
  },
  "revOptions" : {
  }
}
